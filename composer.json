{
  "repositories": [
    {
      "type": "composer",
      "url": "https://connect.advancedcustomfields.com"
    }
  ],
  "require": {
    "timber/timber": "^2.3",
    "wpengine/advanced-custom-fields-pro": "^6.3"
  },
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
